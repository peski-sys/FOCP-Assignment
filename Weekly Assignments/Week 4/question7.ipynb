   "source": [
    "from statistics import mean\n",
    "\n",
    "temps = []\n",
    "\n",
    "for i in range(6):\n",
    "    temp = input(\"Enter temperature (e.g. 25C): \")\n",
    "    value = float(temp[:-1])\n",
    "    temps.append(value)\n",
    "\n",
    "print(\"Maximum:\", max(temps))\n",
    "print(\"Minimum:\", min(temps))\n",
    "print(\"Mean:\", mean(temps))\n"
   ]
