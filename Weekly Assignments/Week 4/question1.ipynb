    "def is_valid_mark(value):\n",
    "    return 0 <= value <= 100\n",
    "\n",
    "\n",
    "num = int(input(\"Enter a number: \"))\n",
    "\n",
    "if is_valid_mark(num):\n",
    "    print(\"Valid number\")\n",
    "else:\n",
    "    print(\"Invalid number\")\n"
